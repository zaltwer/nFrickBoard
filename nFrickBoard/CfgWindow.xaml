<Window x:Name="CfgWindow" x:Class="nFrickBoard.ButtonSetting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="設定" Height="85.29" Width="396" MinHeight="100" MinWidth="300" Topmost="True" WindowStartupLocation="CenterScreen">
    <StackPanel x:Name="config" HorizontalAlignment="Left" Height="79" Margin="0" VerticalAlignment="Top" Width="359" ScrollViewer.VerticalScrollBarVisibility="Disabled">
        <Button x:Name="save" Content="保存" Height="34" Margin="0,0,-0.333,0" Width="100" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="save_Click"/>
        <Button x:Name="cancel" Content="キャンセル" Height="34" Margin="0,0,-0.333,0" Width="100" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="cancel_Click"/>
    </StackPanel>
</Window>
